{
    "[python]": {
        "editor.detectIndentation": false,
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.rulers": [
            81,
            88,
            98
        ],
        "editor.tabSize": 4
    },
    "cSpell.words": [
        "Ansible",
        "CFNgin",
        "CNAME",
        "CPUs",
        "Docstring",
        "FQDN",
        "FQDNs",
        "JWKS",
        "Kubelet",
        "Kubelets",
        "MSYS",
        "NOSPIN",
        "Ngin",
        "PKCE",
        "Serverless's",
        "acmcert",
        "altgraph",
        "amazoncognito",
        "atomicwrites",
        "boto",
        "bufsize",
        "ccpsecuritygroup",
        "certifi",
        "cffi",
        "cfngin",
        "chardet",
        "clidriver",
        "cognito",
        "cpusetcpus",
        "cpython",
        "cygwin",
        "dockerizepip",
        "docutils",
        "entrypoints",
        "epub",
        "executables",
        "fname",
        "fnull",
        "forcerm",
        "gitdb",
        "gitpython",
        "howto",
        "idna",
        "importlib",
        "isort",
        "itertools",
        "jmespath",
        "jsondiff",
        "jsonpatch",
        "jsonpickle",
        "jsonpointer",
        "kustomization",
        "kwarg",
        "lambci",
        "localtoc",
        "macos",
        "markupsafe",
        "mccabe",
        "mybucket",
        "mycdkmodule",
        "myothercdkmodule",
        "mytable",
        "nocache",
        "nondockerizepip",
        "nosniff",
        "onefile",
        "onefolder",
        "openid",
        "overlining",
        "pathlib",
        "pathspec",
        "pefile",
        "pycodestyle",
        "pycparser",
        "pyflakes",
        "pyopenssl",
        "pypa",
        "pypi",
        "pytz",
        "pywin",
        "rxref",
        "semver",
        "sextra",
        "shash",
        "shmsize",
        "skipif",
        "smmap",
        "sshpubkeys",
        "stacker's",
        "stubbers",
        "subdirs",
        "testfixtures",
        "tfvar",
        "tfver",
        "unshallow",
        "urllib",
        "urlshortener",
        "userpool",
        "wontfix",
        "xmltodict",
        "xray",
        "xtrace"
    ],
    "files.insertFinalNewline": true,
    "python.formatting.provider": "black",
    "python.linting.flake8Args": [
        "--docstring-convention=all"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": false,
    "python.linting.pylintEnabled": true,
    "python.pythonPath": "${workspaceFolder}/.venv/bin/python",
    "python.testing.pytestArgs": [
        "tests",
        "--no-cov"
    ],
    "pythonTestExplorer.testFramework": "pytest",
    "restructuredtext.builtDocumentationPath": "${workspaceRoot}/docs/build/html",
    "restructuredtext.confPath": "${workspaceFolder}/docs/source",
    "restructuredtext.linter.extraArgs": [
        "--ignore D001"
    ],
    "restructuredtext.preview.scrollEditorWithPreview": false,
    "restructuredtext.preview.scrollPreviewWithEditor": false,
    "restructuredtext.sphinxBuildPath": "${workspaceFolder}/docs/.venv/bin/sphinx-build",
    "restructuredtext.updateOnTextChanged": "true"
}
